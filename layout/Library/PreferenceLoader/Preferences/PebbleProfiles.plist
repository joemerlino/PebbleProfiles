entry = {
	cell = PSLinkCell;
	icon = "/Library/PreferenceLoader/Preferences/PebbleProfiles.png";
	label = "Pebble Profiles";
	items = (
		{
			cell = PSGroupCell;
			label = "Phone Unlocked Behavior";
			footerText = "When the phone is unlocked, supress notifications from the Pebble.";
		},
		{
			cell = PSSwitchCell;
			label = "Enabled";
			default = 1;
			defaults = "com.tyhoff.pebbleprofiles";
			key = enabled;
			PostNotification = "com.tyhoff.pebbleprofiles.preferencechanged";
		},
		{
			cell = PSGroupCell;
			label = "Allow Notifications From";
			footerText = "These are the applications that will receive notifications. NOTE: These must also be enabled in Settings.";
		},
		{
			bundle = AppList;
			cell = PSLinkCell;
			isController = 1;
			label = "Choose Applications";
			ALSettingsPath = "/var/mobile/Library/Preferences/com.tyhoff.pebbleprofiles.applist.plist";
			ALSettingsKeyPrefix = "";
			ALChangeNotification = "com.tyhoff.pebbleprofiles.preferencechanged";
			"ALSettingsDefaultValue" = 1;
			ALAllowsSelection = 1;
			ALSectionDescriptors = (
				{
				  title = "System Applications";
				  predicate = "isSystemApplication = TRUE";
				  "cell-class-name" = "ALSwitchCell";
				  "icon-size" = 29;
				  "suppress-hidden-apps" = 1;
				},
				{
				  title = "User Applications";
				  predicate = "isSystemApplication = FALSE";
				  "cell-class-name" = "ALSwitchCell";
				  "icon-size" = 29;
				  "suppress-hidden-apps" = 1;
				  "footer-title" = "Â© 2010-2012 Ryan Petrich";
				}
			);
		},
		{
			cell = PSGroupCell;
			isStaticText = 1;
			label = "Credit";
		},
		{
			cell = PSTitleValueCell;
			label = "Pebble Profiles 1.1";
		},
		{
			cell = PSTitleValueCell;
			label = "Made by Tyler Hoffman";
		},
		{
			cell = PSTitleValueCell;
			label = "Contact: tyhoffman1@gmail.com";
		}
	);
};